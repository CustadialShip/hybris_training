				<modifiers read="true" write="true" search="true" optional="true"/>
			</sourceElement>
			<targetElement qualifier="ingredients" type="Ingredient" cardinality="many">
				<modifiers read="true" write="true" search="true" optional="true"/>
			</targetElement>
		</relation>
	</relations>

